{
  "name": "livefromdap-vscode",
  "displayName": "LiveFromDAP",
  "description": "Live programming feedback using DAP",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "livefromdap.showPanel",
        "title": "Show LiveFromDAP Panel"
      },
      {
        "command": "extension.showExecutions",
        "title": "Show Function Executions"
      },
      {
        "command": "extension.loadDataFile",
        "title": "Load Data File"
      },
      {
        "command": "extension.loadDataFileFromContext",
        "title": "Import as Data File"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .jsonl",
          "command": "extension.loadDataFileFromContext",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "socket.io-client": "^4.7.4",
    "axios": "^1.6.7"
  }
} 